{
  "name": "shopify-express-remix-app",
  "version": "1.0.8",
  "description": "A boilerplate for building Shopify apps using Express and Remix",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "config:link": "shopify app config link",
    "config:push": "shopify app config push",
    "dev": "node ./server/index.js",
    "env": "shopify app env",
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "docker-start": "npm run setup && npm run start",
    "shopify": "shopify",
    "prisma": "prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@remix-run/express": "^2.14.0",
    "@remix-run/node": "^2.14.0",
    "@remix-run/react": "^2.14.0",
    "@shopify/app-bridge-react": "^4.1.5",
    "@shopify/polaris": "^13.9.1",
    "@shopify/shopify-app-remix": "^3.4.0",
    "@shopify/shopify-app-session-storage-prisma": "^5.1.5",
    "axios": "^1.7.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "isbot": "^4.1.0",
    "joi": "^17.13.3",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.14.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.4",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "author": "Nazmul Hawlader nazmul291@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nazmul291/shopify-express-remix-app.git"
  },
  "bin": {
    "shopify-express-remix-app": "./bin/create-app.js"
  },
  "keywords": [
    "shopify",
    "boilerplate",
    "shopify-app"
  ],
  "engines": {
    "node": ">=20.0.0"
  }
}
